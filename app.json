{
  "expo": {
    "name": "safekid",
    "slug": "safekid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/shared/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robertw8.safekid"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.robertw8.safekid",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/shared/assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SafeKid to access your camera",
          "microphonePermission": "Allow SafeKid to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "89d38f90-72e5-4911-89fd-5ea912bb71e3"
      }
    }
  }
}
